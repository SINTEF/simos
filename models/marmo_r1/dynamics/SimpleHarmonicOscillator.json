
{	
	"name": "SimpleHarmonicOscillator",
	"type": "SimpleHarmonicOscillator",
	"description": "Properties of a simple harmonic oscillatory function.",
	
    "extends": ["marmo:basic:NamedEntity"],
	
	"properties": [
			
	   {"name": "osciFunc", 
			"type": "string", 
			"value": "sin",
			"description": "oscillatory function, can be either sin, or cos."
			},

			 	  
	   {"name": "T", 
			 "type": "marmo:containers:DimensionalScalar", 
			 "description": "Period",
			 "unit": "s",
			 "value": "1.0"
			 },		
			 
	   {"name": "A", 
			 "type": "marmo:containers:DimensionalScalar", 
			 "description": "Amplitude",
			 "unit": "m"
			 },
			 			 
	   {"name": "b", 
			 "type": "marmo:containers:DimensionalScalar", 
			 "description": "Phase",
			 "unit": "rad"
			 },
			 
	   {"name": "ramp", 
			 "type": "Ramp", 
			 "optional": "true",
			 "description": "oscillator ramp."
			 },
			 
       {"name": "pos", 
            "type": "marmo:containers:NonEquallySpacedSignal", 
            "optional": "true",
            "description": "position signal of the oscillator."},
            
       {"name": "vel", 
            "type": "marmo:containers:NonEquallySpacedSignal", 
            "optional": "true",
            "description": "velocity signal of the oscillator."},
            
       {"name": "acc", 
            "type": "marmo:containers:NonEquallySpacedSignal", 
            "optional": "true",
            "description": "acceleration signal of the oscillator."}            
	 
	]
	
}
