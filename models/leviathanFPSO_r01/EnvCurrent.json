
{   
    "name": "EnvCurrent",
    "type": "EnvCurrent",
    "description": "A current",
    
    "extends": ["marmo:basic:NamedEntity"],
    
    "properties": [
     {"name": "number", 
            "type": "marmo:containers:integer",
			"description": "Number of environment"},
			
	{"name": "Uc", 
            "type": "marmo:containers:DimensionalScalar",
			"unit": "m/s",
			"description": "Current velocity"},
			
	{"name": "pumps", 
            "type": "marmo:containers:SimpleString",  
            "description": "Number and percentage of pumps"},
			
	 {"name": "gauges", 
            "type": "InstrCurrentSensor",  
			"dim":"*",
            "description": "Current sensors installed during current calibration"}
    ]
    
}


