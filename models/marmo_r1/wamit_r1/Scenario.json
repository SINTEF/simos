
{	
	"name": "Scenario",
	"type": "Scenario",
	"description": "Description of a WAMIT scenario.",
	
    "extends": ["marmo:basic:NamedEntity"],
	
	"properties": [
			
	{"name": "poten", 
			"type": "PotenParameters", 
			"description": "poten parameters."},

    {"name": "force", 
            "type": "ForceParameters", 
            "description": "force parameters."},
            
    {"name": "config", 
            "type": "ConfigParameters", 
            "description": "configuration."},
    
    {"name": "results", 
            "type": "Results", 
            "description": "wamit results."},
    
                        
	{"name": "probes", 
			"type": "Probe",
			"dim": "*",
			"description": "a collection of probes."},

	{"name": "postpro", 
			"type": "marmo:containers:DataBundle",
			"description": "a collection of post processed results interesting for this problem."}

	]
	
}
