
{   
    "name": "EnvWave",
    "type": "EnvWave",
    "description": "A wave",
    
    "extends": ["marmo:basic:NamedEntity"],
    
    "properties": [	
	{"name": "spectrum", 
            "type": "marmo:containers:SimpleString",
			"description": "Type of spectrum"},
			
	{"name": "Hs", 
            "type": "marmo:containers:DimensionalScalar",  
			"unit": "m",
            "description": "wave height."},
			
     {"name": "Tp", 
            "type": "marmo:containers:DimensionalScalar",  
			"unit": "s",
            "description": "wave period."},
			
     {"name": "seed", 
            "type": "integer",  
            "value": 1,
            "description": "wave seed."},
			
	{"name": "heading", 
            "type": "marmo:containers:DimensionalScalar",  
            "unit": "deg",
            "description": "Wave heading in global coord"}
			
	{"name": "Wavemaker", 
            "type": "string",  
            "description": "BM2 or BM3"},
			
	{"name": "signal", 
            "type": "marmo:containers:NonEquallySpacedSignal", 
            "description": "an array of a signal to the wavemaker."},
			
     {"name": "gauges", 
            "type": "InstrWaveGauge",  
			"dim":"*",
            "description": "wave gauge ahead of the bulls eye."},

    ]
    
}


