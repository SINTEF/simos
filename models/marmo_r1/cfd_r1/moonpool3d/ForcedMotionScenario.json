
{	
	"name": "ForcedMotionScenario",
	"type": "ForcedMotionScenario",
	"description": "Description of a 3D ship with moonpool forced motion scenario.",
	
    "extends": ["marmo:basic:NamedEntity"],
	
	"properties": [
			
	{"name": "domain", 
			"type": "Domain", 
			"description": "Computational domain."},

	{"name": "oscillator", 
			"type": "marmo:dynamics:SolidBodyOscillator", 
			"description": "surge oscillator."},

	{"name": "forces", 
			"type": "OpenFoamForceCollection", "version": "1",
			"description": "a collection of recorded forces."},

	{"name": "probes", 
			"type": "OpenFoamProbesCollection","version": "1",
			"description": "a collection of probes."},

	{"name": "postpro", 
			"type": "DataBundle","version": "1",
			"description": "a collection of post processed results interesting for this problem."},
									
	{"name": "runConfig", 
			"type": "CFDRunConfig", "version": "1",
			"description": "Run configuration."}

	]
	
}
