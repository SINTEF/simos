
{   
    "name": "InstrWindSensor",
    "type": "InstrWindSensor",
    "description": "Wind velocity sensor",
    
    "extends": ["marmo:basic:NamedEntity"],
    
    "properties": [
	{"name": "sn", 
            "type": "marmo:containers:SimpleString",  
            "description": "Serialnumber"},
	
     {"name": "xcoord", 
            "type": "marmo:containers:DimensionalScalar",  
			"unit": "m",
            "description": "global x coord."},
			
     {"name": "ycoord", 
            "type": "marmo:containers:DimensionalScalar",  
			"unit": "m",
            "description": "global y coord."},
			
	{"name": "zcoord", 
            "type": "marmo:containers:DimensionalScalar",  
			"unit": "m",
            "description": "global z coord."},
			
    {"name": "signal", 
            "type": "marmo:containers:NonEquallySpacedSignal", 
            "description": "an array of a wind sensor signal."}

     ]
    
}


