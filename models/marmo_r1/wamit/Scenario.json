
{	
	"name": "Scenario",
	"type": "Scenario",
	"description": "Description of a WAMIT scenario.",
	
    "extends": ["marmo:project:Scenario"],
	
	"properties": [
			
    {"name": "modelName", 
            "type": "string",
            "value":"temp", 
            "description": "name for the wamit model."},
            
	{"name": "periods", 
			"type": "marmo:containers:EquallySpacedSignal",
			"unit": "s",  
			"description": "wave periods."},

    {"name": "entityList", 
            "type": "string",
            "value": "all",
            "description": "list of relevant entities in the geometry file."},
            			
	{"name": "poten", 
			"type": "PotenParameters", 
			"description": "poten parameters."},

    {"name": "force", 
            "type": "ForceParameters", 
            "description": "force parameters."},
            
    {"name": "config", 
            "type": "ConfigParameters", 
            "description": "configuration."},
    
    {"name": "addedMassDamping", 
            "type": "marmo:dynamics:freqDomain:CompleteAddedMassDamping",
            "description": "all values for added mass and damping mode of motion."},
			
    {"name": "runConfig", 
            "type": "RunConfig", 
            "description": "wamit run configuration."}


	]
	
}
