
{   
    "name": "TestIrrCondition",
    "type": "TestIrrCondition",
    "description": "Irr wave Test condition, with many realizations for the different seed numbers",
    
    "extends": ["marmo:basic:NamedEntity"],
    
	"properties": [
	 {"name": "id", 
			"type": "integer",  
			"description": "test number"},
			
	{"name": "model",
			"type": "dudgeon:Model",
			"description": "model during the tests"},
			
	 {"name": "tests", 
			"type": "TestIrrRealisation", 
			"dim":"*",
			"description": "The seed variations"},
			
	{"name": "calibration", 
			"type": "CalIrrCondition", 
			"description": "The wave calibration condition"},
			
	{"name": "h",
			"type": "marmo:containers:DimensionalScalar",
			"unit": "m",
			"description": "waterdepth"},
			
	{"name": "Hs",
			"type": "marmo:containers:DimensionalScalar",
			"unit": "m",
			"description": "Siginificant waveheight"},
			
	{"name": "Tp",
			"type": "marmo:containers:DimensionalScalar",
			"unit": "s",
			"description": "Peak period"},
			
	{"name": "gamma",
			"type": "marmo:containers:DimensionalScalar",
			"unit": "",
			"description": "gamma parameter of the Jonswap spectrum"}
    ]
    
}


