
{	
	"name": "BilgeKeel",
	"type": "BilgeKeel",
	"description": "Description of a 3D bilge keel geometry.",
	
	"extends": ["marmo:basic:NamedEntityWithUnit"],
	
	"properties": [
			
	{"name": "length", 
			"type": "marmo:containers:DimensionalScalar", 
			"unit": "m",
			"value": "0.0",
			"__dependencies": { "unit": "unit" },
			"description": "bilge keel length"},
												
	{"name": "radius", 
			"type": "marmo:containers:DimensionalScalar", 
			"unit": "m", "value": 0.0,
			"__dependencies": { "unit": "unit" },
			"description": "Bilge radius"},

	{"name": "angle", 
			"type": "marmo:containers:DimensionalScalar", 
			"unit": "deg", "value": 0.0,
			"description": "Bilge keel angle"},
			
	{"name": "height", 
			"type": "marmo:containers:DimensionalScalar", 
			"value": "0.0", "unit": "m",
			"__dependencies": { "unit": "unit" },
			"description": "bilge keel length."},

	{"name": "thickness", 
			"type": "marmo:containers:DimensionalScalar", 
			"value": "0.0", "unit": "m",
			"__dependencies": { "unit": "unit" },
			"description": "bilge keel length."},
			
	{"name": "root", 
			 "type": "marmo:containers:Dimensional3DVector", 
			 "unit": "m",
			 "__dependencies": { "unit": "unit" },
			 "description": "Coordinate for the root of the bilge keel. "},
			 
    {"name": "cen", 
            "type": "marmo:containers:Dimensional3DVector", 
            "unit": "m", 
            "__dependencies": { "unit": "unit" },
            "description": "coordinate for the bilge keel fillet center."},
            
    {"name": "tip", 
            "type": "marmo:containers:Dimensional3DVector", 
            "unit": "m", 
            "__dependencies": { "unit": "unit" },
            "description": "coordinate for the bilge keel tip."},
            
	{"name": "keelShape", 
		"type": "marmo:containers:SimpleString", 
		"value": "plate",
		"description": "describing shape of the bilge keel, can be plate, wedge or etc."}



	]
	
}
