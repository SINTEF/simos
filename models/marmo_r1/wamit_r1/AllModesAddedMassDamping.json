
{	
	"name": "AllModesAddedMassDamping",
	"type": "AllModesAddedMassDamping",
	"description": "Description of WAMIT added mass and damping results.",

	"extends": ["marmo:basic:NamedEntity","MotionModes"],
	
	"properties": [
    
	   {"name": "motionInd", 
			"type": "integer", "from":[1,2,3,4,5,6],
			"value": "1",
			"description": "index for the mode of motion."},
            
       {"name": "surge", 
            "type": "SingleModeAddedMassDamping",
            "__assign": { "forceInd": "1"},
            "__dependencies": { "motionInd": "motionInd"},
            "optional": "true",
            "description": "coefficients in surge."},
            
       {"name": "sway", 
            "type": "SingleModeAddedMassDamping",
            "__assign": { "forceInd": "2"},
            "__dependencies": { "motionInd": "motionInd"},
            "optional": "true",
            "description": "coefficients in sway."},
            
       {"name": "heave", 
            "type": "SingleModeAddedMassDamping",
            "__assign": { "forceInd": "3"},
            "__dependencies": { "motionInd": "motionInd"},
            "optional": "true",
            "description": "coefficients in heave."},
            
       {"name": "roll", 
            "type": "SingleModeAddedMassDamping",
            "__assign": { "forceInd": "4"},
            "__dependencies": { "motionInd": "motionInd"},
            "optional": "true",
            "description": "coefficients in roll."},
            
       {"name": "pitch", 
            "type": "SingleModeAddedMassDamping",
            "__assign": { "forceInd": "5"},
            "__dependencies": { "motionInd": "motionInd"},
            "optional": "true",
            "description": "coefficients in pitch."},
            
       {"name": "yaw", 
            "type": "SingleModeAddedMassDamping",
            "__assign": { "forceInd": "6"},
            "__dependencies": { "motionInd": "motionInd"},
            "optional": "true",
            "description": "coefficients in yaw."}    
            
	]
	
}
