
{	
	"name": "NonEquallySpacedSignal",
	"type": "NonEquallySpacedSignal",
	"description": "data model for a non-equally spaced signal.",
	
	"extends": ["marmo:basic:NamedEntityWithUnit"],
	
	"properties": [
						
	{"name": "nSamples", 
			 "type": "integer", "value": 1,
			 "description": "number of data points."},

	{"name": "xname", 
			"type": "string", "value": "x",
			"description": "name for the x value"},

	{"name": "xdescription", 
			"type": "string", "value": "",
			"description": "name for the x value"},
	 
	{"name": "xunit", 
			 "type": "string", "value": "s",
			 "description": "x signal unit",
			 "simaAttr": "xunit"},
			 			 	  
	{"name": "xvalue", 
			 "type": "double", "dim": "nSamples", 
			 "description": "x data points.",
			 "simaAttr": "xdata"},
			 			 
	{"name": "value", 
			 "type": "double", "dim": "nSamples", 
			 "description": "data points.",
			 "simaAttr": "ydata"}			 

	]
	
}
