
{	
	"name": "FloatingSolidBody",
	"type": "FloatingSolidBody",
	"description": "Dynamic properties of a floating solid body.",
	
    "extends": ["SolidBody", "MotionModes"],
	
	"properties": [
       {"name": "fixedMotionModes", 
             "type": "integer", 
             "dim": "*",
             "description": "the modes which are fixed."},
             
       {"name": "massMatrix", 
             "type": "SixModesMassMatrix",
             "optional": "true", 
             "description": "Mass matrix."},
             
       {"name": "externalRestoringMatrix", 
             "type": "SixModesRestoringMatrix",
             "optional": "true", 
             "description": "restoring matrix."},
             
       {"name": "externalDampingMatrix", 
             "type": "SixModesDampingMatrix",
             "optional": "true", 
             "description": "damping matrix."},
             
	   {"name": "CofB", 
			 "type": "marmo:containers:Dimensional3DVector", 
			 "unit": "m",
			 "__dependencies": { "unit": "unit" },
			 "description": "Coordinate for center of bouyancy. in Body fixed coordinate. "}

	]
	
}
