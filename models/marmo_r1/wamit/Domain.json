
{	
	"name": "Domain",
	"type": "Domain",
	"description": "Description of a WAMIT computation domain.",

    "extends": ["marmo:basic:NamedEntity"],
    	
	"properties": [

	   {"name": "depth", 
			"type": "marmo:containers:DimensionalScalar",
			"value": -1.0,
			"unit": "m",
			"description": "water depth, -1: infinite."},

	   {"name": "uLen", 
			"type": "marmo:containers:DimensionalScalar",
			"value": 1.0,
			"unit": "m",
			"description": "unit length."},
			
	   {"name": "constants", 
			"type": "marmo:dynamics:PhysicalConstants",
			"description": "list of constants."},
			
	   {"name": "rho", 
             "type": "marmo:containers:DimensionalScalar", 
             "value": 1025.0,
             "unit": "kg/m^3",
             "description": "water density."},
             
       {"name": "scale", 
			"type": "double",
			"value": "1.0",
			"description": "Scale of the problem, fullScaleLength/PresentLength."},
			
       	{"name": "fieldPoints", 
			"type": "marmo:containers:VectorArray",
			"optional": "true",
			"description": "coordinates of field points, always come first."},

       	{"name": "fieldLines", 
			"type": "LineGeometry",
			"dim": "*",
			"description": "field data points described by a line."},
			
       	{"name": "fieldGrids", 
			"type": "FieldGeometry",
			"dim": "*",
			"description": "field data points described by polyMesh or GDF type grid."},
			
       	{"name": "body", 
			"type": "Body", 
			"description": "body"}


	]
	
}
