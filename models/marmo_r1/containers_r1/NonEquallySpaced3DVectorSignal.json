
{	
	"name": "NonEquallySpaced3DVectorSignal",
	"type": "NonEquallySpaced3DVectorSignal",
	"description": "data model for a collection of equally spaced 3D vector signals, model test data for instance.",
	
	"extends": ["marmo:basic:NamedEntityWithUnit"],
	
	"properties": [
			
			
	{"name": "nSamples", 
			 "type": "integer", 
			 "value": 1,
			 "description": "number of data points."
			 },
			 			 

	{"name": "xname", 
			"type": "string", "value": "x",
			"description": "name for the x value"},

	{"name": "xdescription", 
			"type": "string", "value": "",
			"description": "name for the x value"},
			 
	{"name": "xunit", 
			 "type": "string", 
			 "value": "",
			 "description": "delta unit."
			 },
			 			 	  
	{"name": "xvalue", 
			 "type": "double", "dim": "nSamples",
			 "description": "independent variable."
			 },
						 			 
	{"name": "x", 
			 "type": "NonEquallySpacedSignal", 
			 "description": "x component value",
			 "__dependencies": {	"unit": "unit",
			 						"xname": "xname",
			 						"xunit": "xunit",
			 						"nSamples": "nSamples",
			 						"xvalue": "xvalue" }
			 },		
			 
	{"name": "y", 
			 "type": "NonEquallySpacedSignal", 
			 "description": "y component value",
			 "__dependencies": {	"unit": "unit",
			 						"xname": "xname",
			 						"xunit": "xunit",
			 						"nSamples": "nSamples",
			 						"xvalue": "xvalue" }
			 },
			 			 
	{"name": "z", 
			 "type": "NonEquallySpacedSignal", 
			 "description": "z component value",
			 "__dependencies": {	"unit": "unit",
			 						"xname": "xname",
			 						"xunit": "xunit",
			 						"nSamples": "nSamples",
			 						"xvalue": "xvalue" }
			 }			 
	]
	
}
