			"type": "marmo:cfd:moonpool3d:Domain", 
			"description": "Computational domain."},

	{"name": "periods", 
			"type": "marmo:containers:EquallySpacedSignal",
			"unit": "s",  
			"description": "wave periods."},
			
    {"name": "directions", 
            "type": "marmo:containers:EquallySpacedSignal",
            "unit": "deg",  
            "description": "wave directions."},
    
            
    {"name": "entityList", 
            "type": "string",
            "value": "all",
            "description": "list of releavent entities in the geometry file."}
            
            

	]
	
}
